     vowel=["a","e","i","o","u"]
    n=input("enter a letter: ")
    count=0
    for i in vowel:
        if i==n:
          count+=1
    if count>0:
        print("vowel")
    else:
        print("not vowel")

#alternate method using short hand if else 

vowel=["a","e","i","o","u"]
n=input("enter a letter: ")
print("its vowel") if n in vowel else print("not a vowel")
