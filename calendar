import random
import calendar
import datetime
import time
li=[1,2,3]
print(sum(li))
print(len(li))
a=random.randint(1,10)
li.append(a)
print(li)
sorted(li)
print(li)
m=6
y=2004
print(calendar.month(y,m))
b=datetime.datetime.now()
print(type(b))
print(time.gmtime())
print(type(calendar.month(y,m)))
print(calendar.monthcalendar(y,m))
print(type(calendar.monthcalendar(y,m)))
